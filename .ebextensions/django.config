option_settings:
    aws:elasticbeanstalk:container:python:
        WSGIPath: mikebozee/wsgi.py

container_commands:
    01_collectstatic:
        command:"source /opt/python/run/mikebozee-env/bin/activate && python mikebozee/manage.py collectstatic --noinput"
    02_migrate:
        command:"source /opt/python/run/mikebozee-env/bin/activate && python mikebozee/manage.py migrate --noinput"

        leader_only: true